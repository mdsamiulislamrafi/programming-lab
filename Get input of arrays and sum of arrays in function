#include <stdio.h>
 void arraysum(int arr[],int n);
 int main(){
    int N,arr[100];
    printf("Enter the value of N:");
    scanf("%d",&N);
    for (int i=0;i<N;i++){
        scanf("%d",&arr[i]);
    }
    arraysum(arr,N);
 return 0;
}
void arraysum(int arr[],int n){
    int sum=0;
    for(int i=0;i<n;i++){
        sum+=arr[i];
    }
    printf("The sum of the elements of array are %d",sum);
    
}
